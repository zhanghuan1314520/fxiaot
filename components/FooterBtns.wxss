.footer-wrap {
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: #fff;
    box-shadow: 0 -4rpx 12rpx 0 rgba(0,0,0,.03);
    padding: 16rpx 32rpx;
    height: 132rpx;
    box-sizing: border-box;
}

.footer-btn {
    flex: 1;
}

.footer-btn+.footer-btn {
    margin-left: 22rpx;
}