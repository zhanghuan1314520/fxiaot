.guide-tips {
    width: auto;
    height: 28rpx;
    font-size: 26rpx;
    font-weight: 400;
    color: #96989e;
    line-height: 28rpx;
    display: flex;
    justify-content: left;
    align-items: center;
    margin: 24rpx 28rpx 0;
}

.guide-tips .new-xianxing-shuoming {
    margin-right: 8rpx;
}

.guide-tips .icon-arrow {
    height: 16rpx;
    width: 16rpx;
    margin-left: 8rpx;
}

.ml-24 {
    margin-left: 32rpx!important;
}

.open-time {
    padding: 0;
    overflow: hidden;
    background: #fff;
    margin-top: 12rpx;
    margin-bottom: 24rpx;
    position: relative;
}

.open-time-container {
    box-sizing: border-box;
}

.list,.open-time-container {
    overflow: hidden;
    height: auto;
}

.list {
    width: auto;
    padding-left: 14px;
}

.list .open-time-item {
    width: 184rpx;
    float: left;
    margin-right: -14rpx;
}

.list .box {
    background-size: cover;
}

.scroll-bar {
    width: 100%;
    height: auto;
    overflow: hidden;
}

.scroll-bar .scroll-bar-container {
    height: 100%;
    overflow: hidden;
    margin-left: 28rpx;
    box-sizing: border-box;
}

.scroll-bar .scroll-bar-container .open-time-item {
    width: 170rpx;
    float: left;
    margin-right: -14rpx;
}

.open-time-item {
    flex: 1;
    height: 136rpx;
    position: relative;
}

.open-time-item:last-child {
    margin-right: 0;
}

.open-time-item .tags {
    width: 84rpx;
    height: 40rpx;
    text-align: center;
    position: absolute;
    right: 8rpx;
    top: 0;
    z-index: 10;
    background: url(https://imgcdn.huanjutang.com/image/2020/03/10/d116596eed62bec50b7852079625a131.png) no-repeat;
    background-position: 100%;
    background-size: contain;
    font-size: 22rpx;
    font-weight: 500;
    color: #fff;
    line-height: 34rpx;
    border: 0;
}

.open-time-item .box {
    margin-top: 16rpx;
    height: 120rpx;
    background-size: cover;
}

.open-time-item .box .label {
    width: auto;
    height: 40rpx;
    font-size: 28rpx;
    font-weight: 700;
    color: rgba(0,0,0,.45);
    line-height: 40rpx;
    padding-top: 28rpx;
    margin-left: 40rpx;
}

.open-time-item .box .date {
    width: auto;
    height: 32rpx;
    font-size: 20rpx;
    font-weight: 400;
    color: rgba(0,0,0,.45);
    line-height: 32rpx;
    margin-left: 40rpx;
    margin-top: 2rpx;
}

