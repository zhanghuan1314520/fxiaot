.share-icon {
    position: fixed;
    right: 32rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 94rpx;
    height: 94rpx;
    background: #fff;
    box-shadow: 0 8px 18px 0 rgba(0,0,0,.14);
    border-radius: 84rpx;
    color: #7f7f7f;
    transform: translateZ(0);
    transition: all .3s linear;
    z-index: 998;
}

.share-icon.moveOut {
    transform: translate3d(95rpx,0,0);
}

.share__button {
    position: absolute;
    width: 100%;
    height: 100%;
    background: transparent;
}