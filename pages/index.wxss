::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.relative {
    position: relative;
}

.tip-bg {
    position: fixed;
    left: 0;
    top: 0;
}

.project-hot-new {
    width: 100%;
    overflow: hidden;
}

.project-hot-new .select-tab {
    line-height: 1;
    overflow: hidden;
}

.project-hot-new .select-tab .list {
    display: flex;
    flex-direction: column;
    align-items: center;
    float: left;
    padding: 50rpx 0 24rpx;
    width: 50%;
    overflow: hidden;
}

.project-hot-new .select-tab .list .text {
    padding-bottom: 15rpx;
    color: #999!important;
}

.project-hot-new .select-tab .list .line {
    width: 60rpx;
    height: 4rpx;
    background-color: transparent;
}

.project-hot-new .select-tab .list.active .text {
    color: #333!important;
}

.project-hot-new .select-tab .list.active .line {
    background-color: #3772cc;
}

.advertise-hide {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.65);
    z-index: 10000;
}

.adertise-box {
    width: 650rpx;
    position: relative;
    border-radius: 12rpx;
    top: 0;
}

.adertise-box>.white-color {
    font-size: 18rpx;
    line-height: 1;
    background: rgba(48,50,55,.4);
    padding: 4rpx 8rpx;
    position: absolute;
    right: 24rpx;
    bottom: 15rpx;
    z-index: 999;
    border-radius: 4rpx;
}

.advertise-hide .advertise {
    background: #fff;
    width: 100%;
    height: 100%;
}

.advertise_close-button {
    position: absolute;
    width: 120rpx;
    height: 60rpx;
    background: rgba(48,50,55,.4);
    border-radius: 8rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    color: hsla(0,0%,100%,.9);
    font-size: 28rpx;
    z-index: 999;
    top: 24rpx;
    right: 24rpx;
    border: 2rpx solid hsla(0,0%,100%,.2);
}

.advertise {
    border-radius: 20rpx;
    position: relative;
    z-index: 223;
    overflow: hidden;
}

.advertise image {
    width: 100%;
    height: 100%;
    display: block;
}

.select-title {
    padding: 48rpx 32rpx 44rpx;
    display: -webkit-box;
    align-items: center;
    display: flex;
    justify-content: flex-start;
    line-height: 1;
}

.select-title--sb {
    justify-content: space-between;
}

.select-title .new-tongyong-gengduo {
    margin-left: 4rpx;
    vertical-align: middle;
}

.select-title .new-wenhaocopy {
    margin-left: 20rpx;
    font-weight: 400rpx;
}

.select-title navigator>text {
    vertical-align: middle;
}

.sales_man {
    padding: 0 32rpx;
    margin-bottom: 10rpx;
    overflow: hidden;
}

.sales_man .sales_man_cont {
    width: 100%;
    display: flex;
    justify-content: space-between;
    overflow: hidden;
}

.sales-man-title {
    position: relative;
    padding-left: 0;
}

.upDataBox {
    display: -webkit-box;
    -webkit-box-pack: center;
    -webkit-box-align: center;
    z-index: 222;
}

.showUpdata-bg,.upDataBox {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
}

.showUpdata-bg {
    background: rgba(0,0,0,.3);
    z-index: 223;
}

.showUpdata {
    width: 600rpx;
    position: relative;
    text-align: center;
}

.showUpdata image {
    width: 100%;
    border-radius: 8rpx;
    left: 0;
    top: 0;
    position: relative;
    z-index: 225;
}

.showUpdata .showUpdata-num {
    padding-top: 140rpx;
}

.showUpdata-cont {
    margin-top: 45rpx;
}

.showUpdata-btn {
    margin-top: 105rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 80rpx;
    border: 1rpx solid #f7776a;
    display: inline-block;
    color: #f7776a;
    padding: 0 140rpx;
    margin-bottom: 52rpx;
}

.showUpdata .upData-cont-box {
    width: 600rpx;
    background: #fff;
    z-index: 224;
    position: relative;
    text-align: relative;
    border-bottom-left-radius: 8rpx;
    border-bottom-right-radius: 8rpx;
    padding: 0rpx 70rpx 0;
    box-sizing: border-box;
    top: -60rpx;
}

.close-btn {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    bottom: -40rpx;
    z-index: 225;
    border: 1rpx solid #fff;
    height: 64rpx;
    line-height: 64rpx;
    width: 64rpx;
    text-align: center;
    border-radius: 64rpx;
}

.page__bd {
    margin-bottom: 0;
    position: relative;
    z-index: 220;
}

.search {
    width: 100%;
    padding: 20rpx 32rpx;
    box-sizing: border-box;
    background: #fff;
}

.nav-search {
    display: flex;
    height: 88rpx;
}

.search-bar-form {
    flex: 1;
    height: 100%;
    width: 100%;
    box-shadow: 0 0 20rpx 0 hsla(0,0%,92.5%,.5);
    border-radius: 8rpx;
    border: 2rpx solid #ededed;
    box-sizing: border-box;
}

.search-bar__form {
    display: flex;
    align-items: center;
    height: 100%;
    width: 100%;
}

.nav-search__input {
    font-size: 28rpx;
    color: rgba(0,0,0,.45);
    margin-left: 12rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    font-weight: 500;
    max-width: 404rpx;
}

.nav-search__op {
    display: flex;
    align-items: center;
    justify-content: flex-start;
    width: 154rpx;
    background: transparent;
}

.nav-search__icon {
    width: 32rpx;
    height: 32rpx;
    margin-left: 28rpx;
}

.nav-search__map {
    font-size: 32rpx;
    font-weight: 500;
    color: rgba(0,0,0,.85);
    margin-left: 6rpx;
    line-height: 36rpx;
}

.nav-search__icon--map {
    color: rgba(0,0,0,.85);
    font-size: 40rpx;
    font-weight: 500;
    margin-left: 32rpx;
}

.newYear-theme {
    background-image: url(https://imgcdn.huanjutang.com/file/2021/01/27/ad08c3875108089e9f2fc918837327be.png);
    background-repeat: no-repeat;
    background-position: 0 calc(var(--status-bar-h) - 2rpx);
    background-size: 100% auto;
    position: relative;
}

.newYear-theme:before {
    content: "";
    background: url(https://imgcdn.huanjutang.com/file/2021/01/30/35fc36a42d4d59e5f90c3753cf80ba9d.png) no-repeat 0 100%/100% auto;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: var(--status-bar-h);
}

.newYear-theme .busi-nav-header {
    transition: none;
}

.newYear-theme .busi-nav-header,.newYear-theme .home__search {
    background: rgba(201,15,5,var(--opacity-top));
}

.newYear-theme .bannerModule {
    position: relative;
    z-index: 3;
}

.newYear-theme .bannerModule:after {
    content: "";
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background: url(https://imgcdn.huanjutang.com/file/2021/01/28/50ea2418e31470386b66e7f69c615576.png) no-repeat 0 0/100% 100%;
    pointer-events: none;
}

.newYear-theme .bannerModule .advert {
    right: 24rpx;
    bottom: 24rpx;
}

.newYear-theme .index-top-search,.newYear-theme .index-top-search .search {
    background: transparent;
}

.newYear-theme .index-top-search .nav-search__icon--map,.newYear-theme .index-top-search .nav-search__map {
    color: #fff;
}

.newYear-theme .index-top-search .search-bar-form {
    box-shadow: none;
    background: hsla(0,0%,100%,.2);
    border-color: transparent;
}

.newYear-theme .index-top-search .nav-search__icon {
    align-self: center;
}

.newYear-theme .modify-color {
    color: hsla(0,0%,100%,.6);
}

.newYear-theme .activityModule {
    margin: -152rpx 0 0;
}

.newYear-theme .activityModule .activity {
    height: 428rpx;
    position: relative;
    z-index: 1;
}

.newYear-theme .activityModule .init__banner-block,.newYear-theme .activityModule .slide-image {
    border-radius: 0;
}

.newYear-theme .activityModule .init__banner-block {
    height: 428rpx!important;
    background-color: transparent;
    overflow: hidden;
}

.newYear-theme .activityModule button {
    background: transparent;
    border-radius: 0;
}

.newYear-theme .grid-top {
    background: url(https://imgcdn.huanjutang.com/file/2021/01/28/19a6b31bf551db930d74234cc9fc933d.png) no-repeat 0 0/750rpx 416rpx;
    height: 416rpx;
    margin-top: -414rpx;
    position: relative;
}

.newYear-theme .grid-middle {
    background: url(https://imgcdn.huanjutang.com/file/2021/01/27/1a2fd4629c1654c2b56fa1fbf0dbf864.png) repeat-y 0 0/100% 100%;
    margin-bottom: -26rpx;
    padding: 0 12rpx;
}

.newYear-theme .grid-middle .grid__name {
    font-size: 24rpx;
}

.newYear-theme .grid-bottom {
    background: url(https://imgcdn.huanjutang.com/file/2021/01/27/2434ecd42fd4a0c733c5afaae173a83d.png) no-repeat 0 100%/750rpx 80rpx;
    height: 50rpx;
}