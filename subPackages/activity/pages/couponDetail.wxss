.lottery-toast {
    position: fixed;
    width: auto;
    padding: 48rpx 60rpx;
    background: rgba(48,50,55,.77);
    border-radius: 8rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    bottom: 500rpx;
    color: #fff;
    left: 50%;
    transform: translate(-50%);
    z-index: 10000;
}

.lottery-toast .cont {
    display: flex;
    align-items: center;
    font-size: 36rpx;
    font-weight: 700;
}

.lottery-toast .image {
    width: 46rpx;
    height: 46rpx;
    margin-right: 18rpx;
}

.lottery-toast .iconnew {
    font-size: 34rpx;
    border-radius: 100%;
    margin-right: 12rpx;
    color: #fff;
}

.lottery-toast .tips {
    width: 332rpx;
    text-align: center;
    margin-top: 30rpx;
}

.unclaimed {
    height: 240rpx;
    width: 686rpx;
    margin: 40rpx 32rpx 10rpx;
    background: url(https://imgcdn.huanjutang.com/image/2020/05/06/d8818fa3e2dbcda3cb60199cc37a57af.png) 50% no-repeat;
    background-size: cover;
}

.unclaimed .info-center {
    left: 226rpx!important;
}

.footer {
    height: 144rpx;
    width: auto;
    background: #fff;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
}

.btn {
    width: auto;
    height: 100rpx;
    background: #2d8cf0;
    border-radius: 8rpx;
    margin: 22rpx 32rpx 0;
    font-weight: 700;
    color: #fff;
    font-size: 32rpx;
    line-height: 100rpx;
}

.disabled {
    background: #d9d9d9!important;
}

.phone-info {
    height: auto;
    overflow: hidden;
    width: 100%;
    text-align: center;
    position: relative;
}

.phone-info .phone {
    width: auto;
    height: 70rpx;
    font-size: 60rpx;
    font-weight: 700;
    color: rgba(0,0,0,.85);
    line-height: 70rpx;
    letter-spacing: 2rpx;
    margin-bottom: 28rpx;
    font-family: DINAlternate-Bold,DINAlternate;
}

.phone-info .hide-eye,.phone-info .show-eye {
    width: 44rpx;
    height: 50rpx;
    position: absolute;
    top: 10rpx;
}

.phone-info .hide-eye .iconnew,.phone-info .show-eye .iconnew {
    font-size: 44rpx;
    color: #bfbfbf;
}

.phone-info .hide-eye {
    right: 80rpx;
}

.phone-info .show-eye {
    right: 66rpx;
}

.phone-info .tip {
    width: auto;
    height: 36rpx;
    font-size: 26rpx;
    font-weight: 400;
    color: rgba(0,0,0,.45);
    line-height: 36rpx;
}

.used {
    width: auto;
    height: 84rpx;
    font-size: 60rpx;
    font-weight: 700;
    color: rgba(0,0,0,.25);
    line-height: 84rpx;
    letter-spacing: 2rpx;
}

.desc {
    overflow: hidden;
    font-size: 24rpx;
    margin: 0 32rpx;
    padding-bottom: 180rpx;
    padding-top: 20rpx;
}

.desc,.range {
    height: auto;
    width: auto;
}

.range {
    color: #f03c50;
    margin: 20rpx 32rpx 40rpx;
    font-size: 28rpx;
    font-weight: 400;
    line-height: 40rpx;
}

.info {
    height: 544rpx;
    width: 690rpx;
    margin-top: 38rpx;
    margin-left: 30rpx;
}

.info-top {
    width: auto;
    height: 244rpx;
    position: relative;
}

.info-top .info-left {
    height: 100rpx;
    overflow: hidden;
    width: 250rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    float: left;
    margin-top: 80rpx;
}

.info-top .info-left .value {
    width: auto;
    font-size: 70rpx;
    font-weight: 700;
    color: #fff;
}

.info-top .info-left .unit {
    width: auto;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 32rpx;
    font-weight: 700;
    color: #fff;
    margin-left: 2rpx;
}

.info-top .info-center {
    position: absolute;
    left: 256rpx;
    top: 50%;
    transform: translateY(-50%);
    width: auto;
    height: auto;
    overflow: hidden;
    display: flex;
    justify-content: center;
    flex-direction: column;
    margin: 0 26rpx;
}

.info-top .info-center .item-name {
    width: 100%;
    height: auto;
    overflow: hidden;
    font-size: 32rpx;
    font-weight: 700;
    color: #fff;
    line-height: 40rpx;
    margin-bottom: 12rpx;
}

.info-top .info-center .item-premise {
    height: auto;
    margin-bottom: 12rpx;
}

.info-top .info-center .item-date,.info-top .info-center .item-premise {
    width: 100%;
    font-size: 24rpx;
    font-weight: 400;
    color: #fff;
    line-height: 32rpx;
}

.info-top .info-center .item-date {
    height: 32rpx;
}

.info-footer {
    width: auto;
    height: 300rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.bg-1 {
    background: url(https://imgcdn.huanjutang.com/image/2020/05/06/1113e1a134c990380ef163add28f7930.png) 50% no-repeat;
    background-size: cover;
}

.bg-2 {
    background: url(https://imgcdn.huanjutang.com/image/2020/05/06/1f62704317ff165bbeab13a8a62c56d0.png) 50% no-repeat;
    background-size: cover;
}

.bg-2 .item-name,.bg-2 .unit,.bg-2 .value {
    color: rgba(0,0,0,.65)!important;
}

.bg-2 .item-date,.bg-2 .item-premise {
    color: rgba(0,0,0,.45)!important;
}

.type-1 .unit {
    margin-top: 24rpx!important;
}

.type-2 .unit {
    margin-top: 28rpx!important;
}

.salesman-info {
    padding-top: 30rpx;
    background: #fff;
    margin-bottom: 20rpx;
    overflow: hidden;
    border-top: 16rpx solid #f5f5f5;
    border-bottom: 16rpx solid #f5f5f5;
}

.salesman-info .common-title {
    padding: 0 24rpx;
}

.salesman-info .common-title .title>.iconnew {
    margin-left: 10rpx;
    vertical-align: middle;
}

.salesman-info .info-container {
    display: flex;
    align-items: center;
    justify-content: left;
    flex-direction: row;
    padding: 28rpx 30rpx 40rpx;
    box-sizing: border-box;
    margin-top: 44rpx;
}

.salesman-info .info-container .item:not(:last-child) {
    margin-right: 30rpx;
}

.salesman-info .info-container .item {
    position: relative;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    flex-direction: column;
    width: 210rpx;
    height: 260rpx;
    background: #fff;
    box-shadow: 0 8rpx 12rpx 0 rgba(48,55,64,.05);
    border-radius: 10rpx;
    border: 1rpx solid;
    border-image: linear-gradient(180deg,rgba(94,104,116,0),rgba(48,55,64,.03)) 2 2;
}

.salesman-info .info-container .item image {
    position: absolute;
    top: -30rpx;
    left: 56rpx;
    display: block;
    width: 100rpx;
    height: 100rpx;
    border-radius: 100rpx;
    border: 2rpx solid rgba(48,55,64,.05);
}

.salesman-info .info-container .item .position {
    position: absolute;
    right: 54rpx;
    top: 50rpx;
}

.salesman-info .info-container .item .name {
    margin: 94rpx 0 22rpx;
    height: 44rpx;
    line-height: 44rpx;
    text-align: center;
    color: #303237;
    font-size: 32rpx;
    font-weight: 500;
}

.salesman-info .info-container .item .btn {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: row;
    width: 138rpx;
    height: 50rpx;
    background: #2f8cef;
    border-radius: 50rpx;
    box-shadow: 0 4rpx 12rpx 0 rgba(47,140,239,.2);
    text-align: center;
    color: #fff;
    line-height: 1;
}

.salesman-info .info-container .item .btn .iconnew {
    margin-right: 16rpx;
}

.common-title {
    display: flex;
    align-items: center;
    justify-content: space-between;
    flex-direction: row;
    box-sizing: border-box;
}

.common-title .title {
    font-size: 32rpx;
    line-height: 50rpx;
    color: rgba(0,0,0,.85);
    font-weight: 700;
}

.common-title .title .iconnew {
    font-weight: 400;
}

.common-title .more {
    font-size: 26rpx;
    color: #96989e;
    display: flex;
    align-items: center;
}

.common-title .more .iconnew {
    margin-left: 8rpx;
    font-size: 22rpx;
    vertical-align: middle;
}

.receiveDialog {
    position: fixed;
    width: auto;
    padding: 36rpx 50rpx;
    background: #fff;
    border-radius: 20rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    bottom: 500rpx;
    color: #fff;
    left: 50%;
    transform: translate(-50%);
    z-index: 10000;
}

.receiveDialog .cont {
    display: flex;
    align-items: center;
    font-size: 36rpx;
    color: #1d1e2c;
    font-weight: 700;
}

.receiveDialog .cont--phone {
    font-size: 56rpx;
    line-height: 56rpx;
    margin-top: 36rpx;
}

.receiveDialog .btn {
    width: 520rpx;
    height: 100rpx;
    margin-top: 30rpx;
    background: #1677ff;
    border-radius: 10rpx;
    font-size: 32rpx;
    color: #fff;
    font-weight: 700;
    display: flex;
    justify-content: center;
    align-items: center;
}

.receiveDialog .image {
    width: 46rpx;
    height: 46rpx;
    margin-right: 18rpx;
}

.receiveDialog .iconnew {
    font-size: 34rpx;
    border-radius: 100%;
    margin-right: 12rpx;
    color: #fff;
}

.receiveDialog .tips {
    height: 34rpx;
    line-height: 34rpx;
    font-size: 24rpx;
    color: #bbb;
    text-align: center;
    margin-top: 40rpx;
    font-weight: 700;
}

.receiveDialog__shade {
    height: 100%;
    width: 100%;
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background: rgba(0,0,0,.65);
    z-index: 2000;
}