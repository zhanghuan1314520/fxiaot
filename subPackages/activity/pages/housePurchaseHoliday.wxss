page {
    background-color: #400088;
}

.page {
    padding-bottom: 50rpx;
}

.page,.page_bg {
    position: relative;
}

.holiday-bg .main-bg {
    width: 100%;
}

.holiday-bg .illustrate {
    position: absolute;
    right: 0;
    top: 40rpx;
    height: 50prx;
    width: 125rpx;
    line-height: 50rpx;
    text-align: center;
    background: rgba(0,0,0,.7);
    border-top-left-radius: 25rpx;
    border-bottom-left-radius: 25rpx;
}

.container {
    position: relative;
    background-color: #6600f9;
    padding: 0 30rpx;
    overflow: hidden;
    box-sizing: border-box;
}

.container .header-nav-list {
    float: left;
    z-index: 10;
    width: 600rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    white-space: nowrap;
    overflow: hidden;
}

.container .header-nav-list .nav-list-item {
    display: inline-block;
    padding: 22rpx 0 20rpx;
    box-sizing: border-box;
    margin-right: 55rpx;
}

.container .header-nav-list .active {
    border-bottom: 4rpx solid #fff976;
    color: #fff976;
    font-weight: 700;
}

.container .choose-all {
    width: 60rpx;
    text-align: right;
    float: right;
    line-height: 80rpx;
    height: 100%;
}

.container-header-bg {
    z-index: 98;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(0,0,0,.3);
}

.container-header-nav {
    background-color: #6600f9;
    z-index: 99;
    position: absolute;
    width: 100%;
    padding: 0 30rpx;
    box-sizing: border-box;
}

.container-header-nav .channel-title {
    float: left;
}

.container-header-nav .choose-all {
    float: right;
}

.container-header-nav .choose-list-show {
    display: flex;
    flex-wrap: wrap;
    margin: 20rpx 0 10rpx;
}

.container-header-nav .choose-list-show .active {
    color: #ffff6f;
}

.container-header-nav .choose-list-show .item-choose-show {
    display: inline-block;
    width: 136rpx;
    text-align: center;
    padding: 17rpx 10rpx;
    box-sizing: border-box;
}

.pur-common-box {
    margin: 40rpx 30rpx 0;
    padding-bottom: 10rpx;
    border-radius: 10rpx;
    overflow: hidden;
    background-color: #6600f9;
}

.pur-common-box .pur-common-top {
    height: 90rpx;
    position: relative;
}

.pur-common-box .pur-common-top .building-name {
    z-index: 2;
    width: 100%;
    position: absolute;
    color: #fff976;
    text-align: center;
}

.pur-common-box .pur-common-top .building-name>view {
    height: 90rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.pur-common-box .pur-common-top .building-name>view>image {
    width: 27rpx;
    height: 30rpx;
}

.pur-common-box .pur-common-top .building-name>view>view {
    line-height: 90rpx;
    display: inline-block;
    overflow: hidden;
    margin: 0 20rpx;
    max-width: 550rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.pur-common-box .pur-common-top>image {
    z-index: 1;
    position: absolute;
    left: 0;
    top: 0;
    height: 90rpx;
    width: 100%;
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
}

.pur-common-box .pur-common-coupon {
    height: 180rpx;
    margin: 30rpx 20rpx 0;
    position: relative;
}

.pur-common-box .pur-common-coupon .coupon-left-money {
    max-width: 460rpx;
    overflow: hidden;
    z-index: 2;
    position: absolute;
    left: 40rpx;
    top: 40rpx;
}

.pur-common-box .pur-common-coupon .coupon-left-bottom {
    max-width: 460rpx;
    overflow: hidden;
    z-index: 2;
    position: absolute;
    left: 40rpx;
    bottom: 40rpx;
}

.pur-common-box .pur-common-coupon .coupon-right {
    z-index: 2;
    position: absolute;
    right: 40rpx;
    top: 67rpx;
    width: 137rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    border-radius: 25rpx;
    background-color: #fff;
    color: #f4502d;
}

.pur-common-box .pur-common-coupon image {
    z-index: 1;
    position: absolute;
    left: 0;
    top: 0;
    height: 180rpx;
    width: 100%;
}

.pur-common-box .pur-common-list {
    background-color: #fff;
    margin: 30rpx 20rpx 0 30rpx;
    padding: 20rpx;
    box-sizing: border-box;
    border-radius: 10rpx;
}

.pur-common-box .pur-common-bottom {
    padding: 30rpx 0;
    text-align: center;
    box-sizing: border-box;
}

.lottery_item_box {
    overflow: hidden;
    position: relative;
}

.lottery_item_box .title_name {
    width: 150rpx;
}

.lottery_item_box .lottery-item-count {
    position: absolute;
    bottom: 32rpx;
    right: 130rpx;
    padding-left: 10rpx;
    z-index: 9;
}

.lottery_item_box .lottery_item_comment {
    float: right;
}

.weui-media-box__title {
    position: relative;
    line-height: 1;
    overflow: hidden;
}

.weui-media-box__title .left {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: break-word;
    word-break: break-all;
    margin-right: 200rpx;
}

.weui-media-box__title .right {
    float: right;
}

.weui-media-box__title .right .height-level-color {
    font-weight: 400;
}

.weui-media-box__title .right .square {
    position: relative;
    top: -3px;
}

.tag {
    font-size: 20rpx;
    line-height: 34rpx;
    padding: 0 10rpx;
    height: 36rpx;
    overflow: hidden;
    color: #1f9f55;
    background-color: #e6fbef;
    border: 0;
}

.weui-media-box {
    display: flex;
    padding-bottom: 20rpx;
    border-bottom: 1px solid #e2e2e2;
}

.weui-media-coupon {
    margin-top: 25rpx;
    overflow: hidden;
    display: flex;
    justify-content: space-between;
}

.weui-media-coupon .coupon-price {
    position: relative;
}

.weui-media-coupon .coupon-price image {
    position: absolute;
    width: 235rpx;
    height: 46rpx;
    top: 0;
}

.weui-media-coupon .getCoupon {
    width: 137rpx;
    height: 46rpx;
    line-height: 46rpx;
    text-align: center;
    border-radius: 5rpx;
    background-color: #f3483b;
}

.weui-media-box__bd {
    margin-left: 35rpx;
}

.lottery_content {
    display: flex;
    align-items: center;
}

.hot_house_img {
    float: left;
    width: 210rpx;
    height: 140rpx;
    position: relative;
}

.hot_house_img .lottery-img {
    display: block;
    width: 210rpx;
    height: 140rpx;
    position: absolute;
    z-index: 1;
    top: 0;
}

.hot_house_img .lottery-img-bottom {
    display: block;
    width: 100rpx;
    height: 30rpx;
    background-color: #f07062;
    line-height: 30rpx;
    text-align: center;
    position: absolute;
    z-index: 2;
    top: 0;
    left: 0;
}

.hot_house_img .view {
    position: absolute;
    bottom: 0;
    left: 10rpx;
    z-index: 3;
}

.hot_house_img .view>text {
    vertical-align: middle;
}

.gray-shadow {
    position: absolute;
    width: 100%;
    height: 120rpx;
    bottom: 0;
    left: 0;
    z-index: 2;
    background: linear-gradient(0deg,rgba(0,0,0,.7),transparent 30%);
}

.application-from {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.6);
    z-index: 198;
}

.application-from-content {
    position: fixed;
    left: 50%;
    top: 50%;
    margin-left: -300rpx;
    margin-top: -330rpx;
    width: 600rpx;
    height: 660rpx;
    border-radius: 10rpx;
    background-color: #fff;
    z-index: 199;
}

.application-from-content .user-input {
    padding: 40rpx 30rpx 0;
}

.application-from-content .input-item {
    border-bottom: 1px solid #e6e6e6;
    padding-bottom: 20rpx;
}

.application-from-content .prompt {
    color: #999;
    padding: 20rpx 30rpx 0;
    box-sizing: border-box;
}

.application-from-content .layout-1 {
    display: flex;
    align-items: center;
    justify-content: center;
}

.application-from-content .layout-2 {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.application-from-content .layout-3 {
    display: flex;
    align-items: center;
}

.application-from-content .get-preferential-btn {
    margin: 65rpx auto 0;
    border-radius: 4rpx;
    width: 550rpx;
    height: 80rpx;
    color: #fff;
    background: #f4502d;
}

.application-from-content .join-rules {
    margin-top: 25rpx;
    text-align: center;
}

.application-from-content image {
    width: 100%;
    height: 346rpx;
}