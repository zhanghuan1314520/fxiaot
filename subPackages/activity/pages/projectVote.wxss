    font-weight: 700;
    color: #ffe752;
    line-height: 44rpx;
    margin-bottom: 20rpx;
}

.vote-counter .vote-line .vote-num-text {
    width: auto;
    height: 28rpx;
    font-size: 28rpx;
    font-weight: 400;
    color: #8c8cec;
    line-height: 28rpx;
}

.search {
    width: 702rpx;
    height: 76rpx;
    line-height: 76rpx;
    background: rgba(91,30,203,.59);
    border-radius: 8rpx;
