}

.activity-content .title {
    position: absolute;
    left: 50%;
    top: 0;
    transform: translate(-50%);
    background: url(https://imgcdn.huanjutang.com/assets/img/201911221556188081.png) no-repeat;
    background-size: cover;
    width: 508rpx;
    height: 114rpx;
