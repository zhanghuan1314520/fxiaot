}

.banner {
    width: 100%;
    height: 746rpx;
    position: relative;
}

.banner image {
    height: 100%;
    width: 100%;
}

.banner .rule {
    width: 150rpx;
    height: 82rpx;
    background: url(https://imgcdn.huanjutang.com/assets/img/20191122152388081.png) no-repeat;
    background-size: cover;
