page {
    background: #f5f7fa;
}

.model-class {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.padding-class {
    padding-bottom: 100rpx;
}

.house_plan_title {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 18rpx 24rpx;
    line-height: 40rpx;
}

.house_plan_title .house_plan_text {
    color: #aeaeae;
}

.house_plan_title .house_plan_del {
    color: #6a82c7;
}

.hou_plan_from {
    background-color: #fff;
    font-size: 30rpx;
}

.general_style {
    display: flex;
    align-items: center;
    position: relative;
}

.text-weight {
    font-weight: 700;
}

.hou_plan_item {
    height: 110rpx;
    padding-left: 30rpx;
    border-bottom: 1px solid #f5f7fa;
}

.hou_plan_item2 {
    border-bottom: none;
}

.hou_plan_item2 .tip-text {
    margin-left: 20rpx;
}

.hou_plan_live {
    flex: 1;
    height: 100%;
    justify-content: flex-end;
    padding-right: 26rpx;
}

.hou_plan_live>input {
    text-align: right;
    margin-right: 24rpx;
    height: 110rpx;
    line-height: 110rpx;
    color: #303237;
    font-weight: 700;
}

.hou_plan_live .text-color {
    flex-shrink: 0;
    font-size: 30rpx;
    font-weight: 400;
    color: #303237;
    height: 110rpx;
    line-height: 110rpx;
}

.hou_plan_live .lottery-name {
    font-size: 30rpx;
    font-weight: 700;
    color: #303237;
    line-height: 110rpx;
}

.hou_plan_live .new-tongyong-gengduo {
    color: #ccc;
    font-size: 24rpx;
    margin-left: 8rpx;
    vertical-align: middle;
}

.l_placeholder {
    font-size: 30rpx;
    font-weight: 400;
    color: #96989e;
}

.general_style2 {
    display: flex;
}

.general_style_font {
    width: 100rpx;
    height: 62rpx;
    background: #edeff2;
    border-radius: 6rpx;
    margin-right: 8rpx;
    margin-left: 22rpx;
    text-align: center;
    color: #303237;
    font-weight: 700;
}

.xian {
    height: 100%;
    display: flex;
    align-items: center;
    flex: 1;
    border-bottom: 1px solid #f5f7fa;
    justify-content: flex-end;
    padding-right: 26rpx;
}

.add_pic {
    width: 102rpx;
    height: 102rpx;
    background-color: #f2f2f2;
    margin-top: 10rpx;
}

.pic_content {
    font-size: 24rpx;
    padding-bottom: 30rpx;
}

.general_style3 {
    display: flex;
    justify-content: center;
    align-items: center;
}

.add_pic_tohouse {
    height: 110rpx;
    width: 100%;
    background-color: #fff;
    margin-top: 12rpx;
    margin-bottom: 80rpx;
}

.add_pic_tohouse .shizi_style {
    margin-right: 10rpx;
    vertical-align: middle;
    margin-top: 2rpx;
}

.add_pic_tohouse .shizi_style,.add_pic_tohouse .upload-text {
    font-size: 32rpx;
    font-weight: 700;
    color: #2d8cf0;
}

.add_submit {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    z-index: 7;
    height: 124rpx;
    background: hsla(0,0%,100%,.95);
    box-shadow: 0rpx -4rpx 12rpx 0rpx rgba(21,24,45,.03);
}

.add_submit .add_submit_item {
    width: 702rpx;
    height: 88rpx;
    background: #2f8cef;
    border-radius: 4rpx;
}

.add_submit .add_submit_text {
    font-size: 32rpx;
    font-weight: 700;
    color: #fff;
    line-height: 124rpx;
}

.upload-file {
    clear: both;
    padding-left: 30rpx;
    height: 210rpx;
}

.upload-file>view {
    float: left;
    margin-right: 5rpx;
}

.upload-file .pic {
    width: 140rpx;
    height: 140rpx;
    box-sizing: border-box;
    margin-top: 10;
    border-radius: 4px;
    position: relative;
    background: url(data:image/png;base64,) 50%/100% no-repeat;
}

.upload-file .pic .dele-icon {
    position: absolute;
    width: 48rpx!important;
    height: 48rpx!important;
    top: -24rpx;
    right: -24rpx;
    z-index: 20;
}

.upload-file .pic .show-image {
    width: 140rpx;
    height: 140rpx;
    border-radius: 10rpx;
}

.upload-file .small-level-color {
    display: inline-block;
    width: 90rpx;
    text-align: center;
    font-size: 20rpx;
    margin-top: 4px;
}

.modalDialog {
    display: block;
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.65);
    z-index: 9;
}

.modalDialog .modalDialog_item {
    width: 606rpx;
    border-radius: 8rpx;
    background: #fff;
    overflow: hidden;
}

.modalDialog .modalDialog_item1 {
    padding: 50rpx 48rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.modalDialog .modalDialog_item2 {
    width: 100%;
    height: 100rpx;
    border-top: 2rpx solid rgba(0,0,0,.1);
}

.modalDialog .modalDialog_style {
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.modalDialog .modalDialog_style2 {
    display: flex;
    flex: 1;
    height: 100%;
    border-left: 2rpx solid rgba(0,0,0,.1);
    justify-content: center;
    align-items: center;
}

.modalDialog .modalDialog_cont {
    font-size: 28rpx;
    color: #303237;
    margin-top: 12px;
    line-height: 24px;
}