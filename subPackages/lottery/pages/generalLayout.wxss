.safe-area {
    padding-bottom: env(safe-area-inset-bottom)!important;
}

page {
    background: #f2f2f2;
}

.general-layout .share-btn {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: row;
    position: fixed;
    top: 24rpx;
    right: 16rpx;
    z-index: 2;
    padding: 0 20rpx;
    height: 50rpx;
    border-radius: 50rpx;
    color: #fff;
    background: rgba(0,0,0,.5);
}

.general-layout .share-btn image {
    width: 32rpx;
    margin-right: 8rpx;
}

.general-layout .scale-view {
    position: relative;
    z-index: 1;
    width: 100%;
    height: 890rpx;
    overflow: hidden;
}

.general-layout .scale-view .move-view {
    position: relative;
}

.general-layout .scale-view .move-view image {
    position: relative;
    width: 960px;
    z-index: 1;
    transition: width .15s,-webkit-transform;
    transition: transform,width .15s;
    transition: transform,width .15s,-webkit-transform;
}

.general-layout .points {
    position: absolute;
    top: 0;
    left: 0;
    z-index: 2;
}

.general-layout .points .point {
    position: absolute;
    color: #303237;
    transition: all .3s;
}

.general-layout .points .point.active {
    color: #fff;
}

.general-layout .points .point.big {
    padding: 0 16rpx 0 20rpx;
    height: 24px;
    line-height: 24px;
    font-size: 15px;
    border-radius: 12px 0 12px 12px;
    background: hsla(0,0%,100%,.9);
}

.general-layout .points .point.big.active {
    background: linear-gradient(1turn,#ee3f4d,#ff5431);
}

.general-layout .points .point.small {
    width: 16px;
    height: 16px;
    line-height: 16px;
    border-radius: 16px;
    font-size: 13px;
    text-align: center;
    background: hsla(0,0%,100%,.9);
}

.general-layout .points .point.small.active {
    background: linear-gradient(1turn,#ee3f4d,#ff5431);
}

.control-view {
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 2;
    width: 100%;
    overflow: hidden;
}

.house-plan {
    position: relative;
    z-index: 2;
    width: 100%;
    overflow-x: scroll;
}

.house-plan,.house-plan .list {
    height: 130rpx!important;
    background: transparent;
}

.house-plan .list .item {
    justify-content: center;
    flex-direction: column;
    width: 584rpx!important;
    height: 130rpx!important;
    border-right: 16rpx solid transparent;
    overflow: hidden;
}

.house-plan .item .title,.house-plan .list .item {
    display: flex;
    align-items: center;
    box-sizing: border-box;
}

.house-plan .item .title {
    width: 100%;
    background: #fff;
    padding: 24rpx 0 0 28rpx;
    border-radius: 4rpx 4rpx 0 0;
    justify-content: flex-start;
    flex-direction: row;
    font-size: 32rpx;
    font-weight: 700;
}

.house-plan .item .title .label {
    height: 32rpx;
    line-height: 32rpx;
    border-radius: 4rpx;
    font-size: 20rpx;
    padding: 0 8rpx;
    margin-left: 16rpx;
    color: #ff7d4f;
    background: rgba(255,125,79,.15);
}

.house-plan .item .info {
    width: 100%;
    padding: 0 0 24rpx 28rpx;
    border-radius: 0 0 4rpx 4rpx;
    background: #fff;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    flex-direction: row;
    flex-wrap: wrap;
    flex: 1;
    box-sizing: border-box;
}

.house-plan .item .info .info-item {
    display: flex;
    align-items: center;
    justify-content: flex-start;
    flex-direction: row;
    font-size: 24rpx;
}

.house-plan .item .info .left {
    height: 34rpx;
    line-height: 34rpx;
    color: #96989e;
}

.house-plan .item .info .right {
    flex: 1;
    height: 34rpx;
    margin-left: 16rpx;
    line-height: 34rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    color: #303237;
}

.house-plan .item.scale .title {
    margin-top: 2rpx;
    padding-top: 22rpx;
}

.house-plan .item.scale .info {
    margin-bottom: 2rpx;
    padding-bottom: 22rpx;
}

.build-content {
    padding: 24rpx 24rpx 0;
    margin: 16rpx 16rpx 0;
    box-sizing: border-box;
    background: #fff;
    max-height: 400rpx;
    overflow-y: scroll;
    overflow-x: hidden;
    padding-bottom: env(safe-area-inset-bottom)!important;
}

.build-content .build-item {
    float: left;
    width: 324rpx;
    height: 164rpx;
    line-height: 72rpx;
    margin: 0 20rpx 24rpx 0;
    padding: 18rpx 24rpx 0;
    background: rgba(150,152,158,.1);
    border-radius: 4rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: #96989e;
    box-sizing: border-box;
}

.build-content .build-item .build-name {
    font-size: 28rpx;
    line-height: 1;
    font-weight: 700;
    color: #303237;
}

.build-content .build-item .tihu {
    padding-top: 26rpx;
    line-height: 1;
}

.build-content .build-item .bottom {
    display: flex;
    align-items: center;
    justify-content: space-between;
    flex-direction: row;
    padding-top: 24rpx;
    line-height: 1;
}

.build-content .build-item .name {
    color: #96989e;
    padding-right: 16rpx;
}

.build-content .build-item .value {
    color: #303237;
}

.build-content .build-item:nth-child(2n) {
    margin-right: 0!important;
}

.build-content .build-item.active {
    background: #2d8cf0;
    box-shadow: 0 8rpx 16rpx 0 rgba(47,140,239,.2);
    color: #fff;
}

.build-content .build-item.active .build-name,.build-content .build-item.active .name,.build-content .build-item.active .value {
    color: #fff;
}

.rooms-content {
    position: relative;
    z-index: 2;
    height: 482rpx;
    margin: 16rpx 16rpx 0;
    background: #fff;
    border-radius: 4rpx;
    overflow: hidden;
    padding-bottom: env(safe-area-inset-bottom)!important;
}

.rooms-content .pager {
    position: relative;
    width: 100%;
    height: 88rpx;
    box-sizing: border-box;
    border-bottom: 2rpx solid rgba(48,55,64,.05);
}

.rooms-content .pager>scroll-view {
    position: relative;
    z-index: 1;
    width: auto;
    height: 100%;
    margin-right: 155rpx;
}

.rooms-content .pager>scroll-view .pager-box {
    display: -webkit-box;
    margin: 0 12rpx;
}

.rooms-content .pager>scroll-view .item {
    position: relative;
    padding: 0 18rpx;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 24rpx;
    color: #96989e;
}

.rooms-content .pager>scroll-view .item.active {
    font-size: 28rpx;
    color: #303237;
}

.rooms-content .pager>scroll-view .item.active:after {
    position: absolute;
    bottom: 0;
    left: 50%;
    content: "";
    transform: translateX(-50%);
    width: 72rpx;
    height: 6rpx;
    background: #2d8cf0;
    border-radius: 3rpx;
}

.rooms-content .pager .back-unit {
    position: absolute;
    top: 0;
    right: 0;
    z-index: 2;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: row;
    height: 100%;
    width: 155rpx;
    padding-left: 15rpx;
    background: transparent url(data:image/png;base64,) 50%/175rpx no-repeat;
}

.rooms-content .pager .back-unit .icon {
    width: 17rpx;
    margin-right: 4rpx;
}

.rooms-content .pager .back-unit text {
    font-size: 24rpx;
    font-weight: 700;
    color: #2d8cf0;
}

.rooms-content .rooms-item,.rooms-content .rooms-list {
    width: 100%;
    height: 394rpx;
    overflow: hidden;
}

.rooms-content .room-container {
    width: 100%;
    height: 286rpx;
    padding: 8rpx 0;
    box-sizing: border-box;
    overflow-y: scroll;
}

.rooms-content .room-container .item {
    display: block;
    padding: 26rpx 0;
    margin: 0 32rpx;
    box-sizing: border-box;
    border-bottom: 2rpx solid rgba(48,55,64,.05);
    overflow: hidden;
}

.rooms-content .room-container .item .cover {
    float: left;
    width: 188rpx;
    height: 140rpx;
    background: #d8d8d8;
    border-radius: 4rpx;
    border: 2rpx solid #f4f4f5;
    overflow: hidden;
}

.rooms-content .room-container .item .right {
    display: block;
    margin-left: 212rpx;
    overflow: hidden;
}

.rooms-content .room-container .item .right .title {
    height: 28rpx;
    font-size: 28rpx;
    font-weight: 400;
    color: #303237;
    line-height: 28rpx;
    margin: 4rpx 0 16rpx;
}

.rooms-content .room-container .item .right .huxing {
    height: 24rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: #96989e;
    line-height: 24rpx;
}

.rooms-content .see-house {
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    margin: 0 24rpx 20rpx;
    background: rgba(47,140,239,.05);
    border-radius: 4rpx;
    border: 2rpx solid rgba(47,140,239,.15);
    color: #2d8cf0;
    font-size: 28rpx;
    font-weight: 700;
}

.house-plan,.rooms-content {
    transform: translateZ(0);
    transition: transform .5s;
    transition: transform .5s,-webkit-transform .5s;
}