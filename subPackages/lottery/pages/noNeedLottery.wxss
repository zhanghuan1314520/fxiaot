.lottery_item_box {
    padding: 0 24rpx;
    overflow: hidden;
}

.weui-media-box {
    padding: 24rpx 0;
    border-bottom: 2rpx solid #f2f2f2;
    overflow: hidden;
}

.hot_house_img {
    float: left;
    width: 214rpx;
    height: 156rpx;
    position: relative;
    border-radius: 10rpx;
}

.hot_house_img .project-hasVideo {
    position: absolute;
    width: 70rpx;
    height: 70rpx;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    z-index: 10;
}

.hot_house_img .lottery-img {
    width: 100%;
    height: 100%;
    position: relative;
    border-radius: 10rpx;
}

.hot_house_img .gray-shadow {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 2;
    width: 100%;
    height: 100%;
    background: linear-gradient(180deg,transparent,rgba(0,0,0,.51));
    border-radius: 10rpx;
}

.hot_house_img .view {
    position: absolute;
    left: 12rpx;
    bottom: 4rpx;
    z-index: 3;
    font-weight: 400;
    color: #fff;
    line-height: 34rpx;
    opacity: .8;
}

.hot_house_img .view .icon {
    position: relative;
    margin-right: 6rpx;
    top: 2rpx;
}

.hot_house_img .hot-rank {
    position: absolute;
    z-index: 5;
    top: 0;
    left: 10rpx;
    width: 44rpx;
    height: 46rpx;
    color: #fff;
    text-align: center;
    font-size: 32rpx;
    font-weight: 700;
    background-image: url(https://imgcdn.huanjutang.com/assets/img/2019117104077887.png);
    background-size: 100% 100%;
}

.hot_house_img .hot-rank.top {
    background-image: url(https://imgcdn.huanjutang.com/assets/img/2019117104078081.png);
}

.weui-media-box__bd {
    margin-left: 240rpx;
    overflow: hidden;
}

.weui-media-box__bd .weui-media-box__title {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    font-size: 30rpx;
    font-weight: 500;
    color: #303237;
    line-height: 30rpx;
    margin-bottom: 16rpx;
}

.weui-media-box__bd .price {
    font-size: 28rpx;
    font-weight: 500;
    color: #ee3f4d;
    line-height: 28rpx;
    margin-bottom: 12rpx;
}

.weui-media-box__bd .area-price,.weui-media-box__bd .area-price .push_time {
    font-size: 24rpx;
    font-weight: 400;
    color: #96989e;
    line-height: 24rpx;
    margin-bottom: 12rpx;
}

.weui-media-box__bd .area-price .push_time {
    margin-left: 20rpx;
}

.weui-media-box__bd .list_bottom {
    display: flex;
    align-items: center;
    justify-content: space-between;
    flex-direction: row;
}

.weui-media-box__bd .list_bottom .left {
    font-size: 28rpx;
    font-weight: 400;
    color: #ee3f4d;
    line-height: 40rpx;
}

.weui-media-box__bd .left .line-tag {
    display: inline-block;
    float: left;
    background: rgba(47,140,239,.15);
    color: #2f8cef;
    font-size: 20rpx;
    font-weight: 400;
    border-radius: 4rpx;
    padding: 0 4rpx;
    height: 34rpx;
    line-height: 34rpx;
    margin-right: 8rpx;
}

.weui-media-box__bd .left .line-tag-1 {
    color: #ff7d4f;
    background: rgba(255,125,79,.15);
}

.weui-media-box__bd .left .list_tag {
    background: rgba(10,191,121,.15);
    color: #0abf79;
    margin-right: 10rpx;
}

.weui-media-box__bd .list_bottom .lottery_item_comment {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: row;
}

.weui-media-box__bd .list_bottom .comment-num {
    margin-left: 10rpx;
}

.img-tags .img-tag-item .image {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.bannerModule {
    margin: 16rpx 32rpx;
}

.haveSearchBar {
    padding-top: 128rpx;
}

.haveSearchBar .search {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 999;
}

.ad-banner {
    height: 300rpx;
}

.advert {
    font-size: 18rpx;
    line-height: 26rpx;
    text-align: center;
    background: rgba(0,0,0,.3);
    width: 50rpx;
    height: 26rpx;
    position: absolute;
    right: 20rpx;
    bottom: 15rpx;
    z-index: 222;
    border-radius: 1rpx;
    opacity: .4;
}

.slide-image {
    width: 100%;
    max-width: 100%;
    height: 350rpx;
}

.projects {
    position: relative;
    z-index: 1;
    width: 100%;
    overflow: hidden;
    padding: 40rpx 32rpx;
    box-sizing: border-box;
}

.not_id_card {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    margin-top: 220rpx;
}

.not_id_card image {
    width: 358rpx;
    height: 224rpx;
}

.not_id_card text {
    margin-top: 46rpx;
    color: #6a7490;
    font-size: 32rpx;
}

.flex_layout_c {
    width: 100%;
    margin-top: 30rpx;
    margin-bottom: 30rpx;
}

.empty_block {
    background-color: #f2f2f2;
    height: 100rpx;
}

.fixed-top {
    position: fixed;
    top: 128rpx;
    left: 0;
    z-index: 99;
    width: 100%;
}

.filter-block {
    height: 78rpx;
    background: #fff;
}

.img-tags {
    height: 100%;
    width: 100%;
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    text-align: center;
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 3;
}

.img-tags .img-tag-item {
    width: 58rpx;
    height: 58rpx;
    border-radius: 50%;
    display: inline-block;
    margin: 0 3rpx;
}

.img-tags .img-tag-item image {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.h-200 {
    height: 200rpx!important;
}

.zIndex-3 {
    z-index: 3;
}

.project-map_entry {
    position: fixed;
    bottom: 60rpx;
    right: 8rpx;
    width: 136rpx;
    height: 136rpx;
    border-radius: 50%;
    background: url(https://imgcdn.huanjutang.com/assets/img/20191113175878081.png);
    background-size: 100% 100%;
    z-index: 99;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    color: #2f8cef;
    font-size: 28rpx;
    font-weight: 700;
}

.project-map_entry>text:last-child {
    position: relative;
    top: -4rpx;
}

.project-map_entry .iconnew {
    font-size: 40rpx;
    position: relative;
    top: -2rpx;
}

.project-menu-fixed {
    position: fixed;
    z-index: 40;
}

.project-nav-fixed {
    top: 100rpx;
    z-index: 30;
    position: fixed;
}

.content-intro {
    line-height: 56rpx;
    background: #fff6df;
    margin: 16rpx 32rpx 0;
    border-radius: 8rpx;
    padding: 12rpx 20rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
}

.content-intro .content {
    flex: 1;
    width: 624rpx;
    line-height: 32rpx;
    color: #99651f;
}

.content-intro .icon {
    line-height: 32rpx;
    margin-left: 18rpx;
    width: 32rpx;
    color: #99651f;
}

.short-margin {
    margin: 16rpx 32rpx 0;
    border-radius: 8rpx;
}

.projects__filters {
    position: sticky;
    top: 0;
    left: 0;
    z-index: 98;
    width: 100%;
    height: 88rpx;
}

.projects__filters .projects__filters-title {
    padding-left: 34rpx;
    color: rgba(0,0,0,.85);
    font-weight: 700;
    line-height: 52rpx;
    margin-bottom: 8rpx;
}

.projects__filters .filters__fixed {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 99;
    width: 100%;
}

.projects__filters .filters__bg {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 98;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.2);
    opacity: 0;
    transform: translateY(-100%);
    transition: opacity .3s;
}

.projects__filters .filters__bg.filters__bg-active {
    opacity: 1;
    transform: translateY(0);
}