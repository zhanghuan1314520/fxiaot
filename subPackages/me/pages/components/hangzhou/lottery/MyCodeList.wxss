.my-code-container {
    padding: 132rpx 0 150rpx;
    box-sizing: border-box;
    background: #f5f5f5;
    overflow: hidden;
}

.my-code-container .list {
    position: relative;
    padding: 40rpx 32rpx;
    box-sizing: border-box;
    border-radius: 8rpx;
    overflow: hidden;
    width: 686rpx;
    background: #fff;
    margin: 0 auto 28rpx;
}

.my-code-container .list .project-info {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 28rpx;
    height: 40rpx;
    overflow: hidden;
}

.my-code-container .list .project-info>.title {
    position: relative;
    display: flex;
    align-items: center;
    justify-content: flex-start;
}

.my-code-container .list .project-info>.title .name {
    display: block;
    color: rgba(0,0,0,.85);
    font-size: 36rpx;
    font-weight: 700;
    line-height: 40rpx;
    max-width: 260rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.my-code-container .list .project-info>.title .turn {
    text-align: center;
    padding: 0 8rpx;
    height: 40rpx;
    background: rgba(45,140,240,.1);
    border-radius: 2rpx;
    font-size: 24rpx;
    color: #2d8cf0;
    line-height: 40rpx;
    margin-left: 12rpx;
}

.my-code-container .list .project-info>.title .turn.data-error {
    padding: 0 12rpx;
    background: rgba(10,180,100,.12);
    color: #0ab464;
}

.my-code-container .list .project-info .right-operations {
    display: flex;
    align-items: center;
    line-height: 40rpx;
}

.my-code-container .list .project-info .right-operations .line-split {
    width: 1rpx;
    background: rgba(0,0,0,.1);
    height: 28rpx;
    display: inline-block;
}

.my-code-container .list .project-info .right-operations>.iconnew {
    color: rgba(0,0,0,.65);
    opacity: .61;
}

.my-code-container .list .project-info .right-operations>.iconnew.new-shanchu {
    margin: 0 28rpx 0 32rpx;
}

.my-code-container .list .info-line {
    display: flex;
    align-items: center;
    justify-content: flex-start;
}

.my-code-container .list .info-line>.li {
    overflow: hidden;
    display: flex;
    align-items: center;
    line-height: 32rpx;
}

.my-code-container .list .info-line .line-split {
    width: 1px;
    background: rgba(0,0,0,.1);
    height: 28rpx;
    display: inline-block;
    margin: 0 28rpx;
}

.my-code-container .list .info-line .left {
    display: inline-block;
    text-align: left;
    color: rgba(0,0,0,.45);
}

.my-code-container .list .info-line .right {
    color: rgba(0,0,0,.85);
}

.my-code-container .list>.status {
    position: absolute;
    right: 0;
    top: 24rpx;
    padding: 0 24rpx 0 44rpx;
    line-height: 62rpx;
    border-radius: 62rpx 0 0 62rpx;
    overflow: hidden;
    color: #ee3f4d;
    background: rgba(238,63,77,.06);
}

.my-code-container .list>.status.fuhe {
    color: #96989e;
    background: #f2f4f8;
}

.my-code-container .list>.status.jieguo {
    color: rgba(35,205,148,.98);
    background: rgba(35,205,148,.06);
}

.my-code-container .list>.delete {
    position: absolute;
    bottom: 0;
    right: 0;
    width: 68rpx;
    height: 68rpx;
    text-align: center;
}

.my-code-container .list>.delete text {
    position: absolute;
    bottom: 8rpx;
    right: 8rpx;
    z-index: 2;
}

.my-code-container .list>.delete:after {
    position: absolute;
    right: 0;
    bottom: 0;
    z-index: 1;
    content: "";
    width: 0;
    height: 0;
    border-bottom: 68rpx solid #b5b6bb;
    border-left: 68rpx solid transparent;
}

.my-code-container>.bg {
    display: block;
    max-width: 382rpx;
    padding: 84rpx 0 42rpx;
    margin: 0 auto;
}

.my-code-container>.lottery_h2,.my-code-container>.lottery_p {
    text-align: center;
}

.my-code-container>.lottery_h2 {
    font-weight: 700;
    margin-bottom: 32rpx;
    line-height: 36rpx;
}

.my-code-container>.add-btn {
    position: relative;
    display: block;
    margin: 92rpx auto 80rpx;
    width: 576rpx;
    line-height: 88rpx;
    border-radius: 44rpx;
    text-align: center;
    background: #2d8cf0;
    box-shadow: 0 12rpx 20rpx 0 rgba(45,140,240,.3);
    color: #fff;
    font-weight: 700;
}

.no-content {
    position: fixed;
    width: 100%;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    text-align: center;
}

.no-content image {
    width: 406rpx;
    margin-bottom: 52rpx;
    height: 224rpx;
}

.no-content .no-result {
    color: rgba(0,0,0,.85);
    font-size: 36rpx;
    margin-bottom: 22rpx;
    line-height: 48rpx;
    font-weight: 700;
}

.no-content .add-user-tips {
    color: rgba(0,0,0,.45);
    font-size: 28rpx;
}

.bottom-btns {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    background: #fff;
    padding-bottom: env(safe-area-inset-bottom);
}

.bottom-btns .button-add-user {
    background: #fff;
    box-shadow: 0 -4rpx 12rpx 0 rgba(0,0,0,.03);
    height: 144rpx;
    margin: 0 32rpx;
    display: flex;
    align-items: center;
    box-sizing: border-box;
}

.bottom-btns .button-add-user .button {
    width: 100%;
    height: 100rpx;
    background: #2d8cf0;
    border-radius: 8rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #fff;
    font-size: 32rpx;
    font-weight: 700;
}