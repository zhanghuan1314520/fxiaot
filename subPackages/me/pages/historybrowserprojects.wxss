.history-browser-projects .search-bar {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100rpx;
    box-sizing: border-box;
    padding: 8px 20rpx 0;
    background-color: #2d8cf0;
    z-index: 999;
}

.history-browser-projects .search-bar .search-bar-form {
    background: #fff;
    line-height: 100%;
    color: #ccc;
    position: relative;
    margin: 0 10rpx 20rpx;
    z-index: 1;
    overflow: hidden;
    border-radius: 66rpx;
}

.history-browser-projects .search-bar .search-bar-form .weui-icon-clear,.history-browser-projects .search-bar .search-bar-form .weui-icon-search {
    width: 76rpx;
    height: 66rpx;
    line-height: 66rpx;
    text-align: center;
    font-size: 38rpx;
    color: #ccc;
    padding: 0;
}

.history-browser-projects .search-bar .search-bar-form .search-bar-input {
    width: auto!important;
    margin: 0 76rpx;
    height: 66rpx;
    color: #666;
}

.history-browser-projects .search-bar .search-bar-form .weui-icon-clear {
    position: relative;
    float: right;
    z-index: 2;
    left: 0;
}

.history-browser-projects .search-bar .search-bar-form .weui-icon-search {
    position: relative;
    float: left;
    z-index: 2;
    right: 0;
    margin-right: 0;
}

.history-browser-projects .search-bar .search-bar-icon {
    width: 70rpx;
    text-align: center;
    z-index: 2;
    line-height: 1;
    background-color: transparent;
    overflow: hidden;
}

.history-browser-projects .search-bar .float-left {
    float: left;
}

.history-browser-projects .search-bar .float-right {
    float: right;
}

.history-browser-projects .search-bar .search-bar-icon button.e_btn {
    width: 100%;
    height: 44rpx;
    line-height: 44rpx;
    padding: 0;
    background-color: transparent;
    font-size: 44rpx;
}

.history-browser-projects .search-bar .search-bar-icon text {
    line-height: 1;
}

.history-browser-projects .search-bar .search-bar-icon button::after {
    display: none;
}

.history-browser-projects .search-bar .search-input-place {
    color: #ccc;
}

.history-browser-projects .search-bar .search-btn {
    float: right;
    height: 70rpx;
    width: 80rpx;
    line-height: 70rpx;
    text-align: center;
    padding-left: 15rpx;
}

.history-browser-projects .projects {
    padding: 24rpx 32rpx;
}

.history-browser-projects .nothing {
    margin-top: 102rpx;
    text-align: center;
}

.history-browser-projects .nothing image {
    display: inline-block;
    width: 276rpx;
    height: 144rpx;
}

.history-browser-projects .nothing image.article-nothing {
    width: 276rpx;
    height: 176rpx;
}

.history-browser-projects .nothing .text {
    margin-top: 48rpx;
    padding-bottom: 124rpx;
}

.history-browser-projects .nothing .content {
    margin-top: 48rpx;
}

.bottom-line {
    text-align: center;
    padding: 20rpx 0 50rpx;
}