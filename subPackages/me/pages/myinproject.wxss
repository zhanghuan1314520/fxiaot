page {
    background: #f2f2f2;
}

.my-in-project__wrapper {
    position: relative;
    padding-bottom: 144rpx;
}

.my-in-project__wrapper .tool-box {
    height: 56rpx;
    padding: 40rpx 32rpx 0rpx;
}

.my-in-project__wrapper .tool-box .tit {
    color: rgba(0,0,0,.85);
    font-weight: 700;
}

.my-in-project__wrapper .tool-box .icon {
    color: #2d8cf0;
    margin-right: 12rpx;
    font-weight: 500;
}

.my-in-project__wrapper .tool-box .tool {
    color: rgba(0,0,0,.85);
}

.my-in-project__wrapper .lottery-card {
    width: 686rpx;
    background: #fff;
    border-radius: 8rpx;
    margin: 24rpx 32rpx 0;
    padding: 28rpx 28rpx 0;
    box-sizing: border-box;
}

.my-in-project__wrapper .lottery-card .main-info {
    display: flex;
    position: relative;
}

.my-in-project__wrapper .lottery-card .main-info .main-tag {
    position: absolute;
    left: 0;
    top: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 64rpx;
    height: 36rpx;
    background: #ff6908;
    border-radius: 8rpx 8rpx 8rpx 0rpx;
    font-size: 24rpx;
    font-weight: 700;
    color: #fff;
}

.my-in-project__wrapper .lottery-card .img {
    width: 180rpx;
    height: 140rpx;
    border-radius: 8rpx;
    margin-right: 24rpx;
    flex-shrink: 0;
}

.my-in-project__wrapper .lottery-card .info-box {
    width: 100%;
}

.my-in-project__wrapper .lottery-card .info-box .name {
    width: 100%;
    color: rgba(0,0,0,.85);
    line-height: 50rpx;
    font-weight: 700;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
}

.my-in-project__wrapper .lottery-card .info-box .location {
    width: 100%;
    color: rgba(0,0,0,.45);
    line-height: 40rpx;
    font-weight: 400;
    margin-top: 8rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.my-in-project__wrapper .relation-btn {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100vw;
    height: 144rpx;
    background: #fff;
    box-shadow: 0rpx -4rpx 12rpx 0rpx rgba(0,0,0,.03);
    text-align: center;
    padding-bottom: env(safe-area-inset-bottom);
}

.my-in-project__wrapper .relation-btn .btn {
    margin-left: 32rpx;
    width: 686rpx;
    height: 100rpx;
    line-height: 100rpx;
    background: #2d8cf0;
    border-radius: 8rpx;
    font-weight: 700;
    color: #fff;
}

.my-in-project__wrapper .no-gary {
    color: #bfbfbf!important;
}

.my-in-project__wrapper .no-gary-txt {
    color: #606266!important;
}

.my-in-project__wrapper .operate {
    margin-top: 28rpx;
    padding: 24rpx 0 28rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-top: 1rpx solid rgba(0,0,0,.07);
}

.my-in-project__wrapper .operate .text {
    font-size: 28rpx;
    color: rgba(0,0,0,.45);
    line-height: 40rpx;
}

.my-in-project__wrapper .operate .iconnew {
    width: 40rpx;
    height: 40rpx;
    color: rgba(0,0,0,.07);
    font-size: 46rpx;
}

.my-in-project__wrapper .operate .new-checked {
    position: relative;
    top: 4rpx;
    left: 6rpx;
    font-size: 36rpx;
    color: #2d8cf0;
}

.my-in-project__wrapper .tip {
    margin: 24rpx 32rpx 40rpx;
    line-height: 40rpx;
    white-space: pre-line;
    font-size: 24rpx;
    color: rgba(0,0,0,.45);
}