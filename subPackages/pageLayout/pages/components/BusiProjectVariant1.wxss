.main {
    background: #f5f5f7;
}

.projects__filters {
    position: sticky;
    top: 0;
    left: 0;
    z-index: 98;
    width: 100%;
    height: 88rpx;
}

.projects__filters .projects__filters-title {
    padding-left: 34rpx;
    color: rgba(0,0,0,.85);
    font-weight: 700;
    line-height: 52rpx;
    margin-bottom: 8rpx;
}

.projects__filters .filters__fixed {
    z-index: 99;
}

.filters__bg {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.2);
    opacity: 0;
    transform: translateY(-100%);
    transition: opacity .3s;
}

.filters__bg.filters__bg-active {
    opacity: 1;
    transform: translateY(0);
}

.relative {
    position: relative;
}

.not_id_card {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    margin-top: 220rpx;
}

.not_id_card .image {
    width: 358rpx;
    height: 224rpx;
}

.not_id_card .text {
    margin-top: 46rpx;
    color: #6a7490;
    font-size: 32rpx;
}

.advert {
    font-size: 18rpx;
    line-height: 26rpx;
    background: rgba(0,0,0,.3);
    width: 50rpx;
    height: 26rpx;
    right: 12rpx;
    bottom: 12rpx;
    z-index: 222;
    border-radius: 1rpx;
    opacity: .4;
}

.advert,.hot-rank {
    text-align: center;
    position: absolute;
}

.hot-rank {
    z-index: 5;
    top: 0;
    left: 10rpx;
    width: 44rpx;
    height: 46rpx;
    color: #fff;
    font-size: 32rpx;
    font-weight: 700;
    background-image: url(https://imgcdn.huanjutang.com/assets/img/2019117104077887.png);
    background-size: 100% 100%;
}

.hot-rank--red {
    background-image: url(https://imgcdn.huanjutang.com/assets/img/2019117104078081.png);
}

.line {
    display: inline-block;
    width: 2rpx;
    height: 20rpx;
    margin: 0 10rpx;
    background: #bfbfbf;
}

.middle__item {
    width: 686rpx;
    box-sizing: border-box;
    margin-left: 32rpx;
    padding: 24rpx;
    border-radius: 12rpx;
    margin-top: 32rpx;
    background: #fff;
}

.middle__item:nth-child(1) {
    border-top: none;
    margin-top: 0;
}

.project__img {
    position: relative;
}

.project__headIcon,.project__img {
    width: 100%;
    height: 300rpx;
    border-radius: 8px 8px 0 0;
}

.project__headIcon {
    background: #c5c3c3;
}

.project__view {
    position: absolute;
    display: flex;
    line-height: 24rpx;
    bottom: 16rpx;
    left: 12rpx;
    opacity: .8;
}

.project__view__icon {
    color: #fff;
    vertical-align: middle;
}

.project__view__text {
    color: hsla(0,0%,100%,.8);
}

.project__info__spot {
    display: flex;
    align-items: center;
    margin-top: 12rpx;
}

.project__info__spot .light__icon {
    width: 32rpx;
    height: 32rpx;
}

.project__info__spot .light__title {
    font-size: 24rpx;
    font-family: PingFangSC-Medium,PingFang SC;
    color: rgba(0,0,0,.85);
}

.project__info__spot .light__spot {
    text-align: right;
    font-size: 24rpx;
    color: rgba(0,0,0,.85);
    width: 400rpx;
}

.project__info__spot:nth-child(1) {
    margin-top: 22rpx;
}

.project__info__spot--sm {
    margin-top: 16rpx;
}

.project__info__spot .light__icon,.project__info__spot .light__spot,.project__info__spot .light__title,.project__info__spot .line {
    vertical-align: middle;
}

.feature__tips__item {
    height: 32rpx;
    line-height: 32rpx;
    padding: 0 8rpx;
    background: #eceff5;
    border-radius: 4rpx;
    color: #828caa;
    margin-left: 12rpx;
    font-size: 22rpx;
    display: inline-block;
}

.feature__tips__item:nth-child(1) {
    margin-left: 0;
}

.info__tips__item {
    display: inline-block;
    margin-left: 12rpx;
    padding: 0rpx 8rpx;
    height: 32rpx;
    line-height: 32rpx;
    border-radius: 4rpx;
    font-size: 24rpx;
    font-weight: 400;
}

.info__tips__item,.info__tips__item--zs {
    background: rgba(45,140,240,.12);
    color: #2d8cf0;
}

.info__tips__item--jjys {
    background: rgba(10,180,100,.12);
    color: #0ab464;
}

.info__tips__item--zxyh {
    background: rgba(45,140,240,.12);
    color: #2d8cf0;
}

.info__tips__item--zzgs {
    background: rgba(250,120,50,.12);
    color: #fa7832;
}

.info__tips__item--yqp {
    background: rgba(240,60,80,.12);
    color: #f03c50;
}

.info__tips__item--fhmd {
    background: rgba(250,120,50,.12);
    color: #fa7832;
}

.info__tips__item:nth-child(1) {
    margin-left: 0;
}

.project__info__price {
    display: block;
    font-size: 32rpx;
    font-weight: 700;
    color: #f03c50;
    line-height: 40rpx;
    margin-top: 4rpx;
}

.project__info__price:nth-child(1) {
    margin-top: 0rpx;
}

.project__info__price--1 {
    height: 44rpx;
    font-size: 32rpx;
    line-height: 44rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.project__info__price--2 {
    height: 36rpx;
    font-size: 28rpx;
    font-weight: 700;
    line-height: 36rpx;
}

.project__info__price--3 {
    height: 32rpx;
    font-size: 24rpx;
    font-weight: 700;
    color: #f03c50;
    line-height: 32rpx;
}

.project__info__location {
    width: 300rpx;
}

.project__info__top {
    margin-top: 28rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.card__info__name {
    font-size: 34rpx;
    line-height: 44rpx;
    width: 400rpx;
    color: #2e3135;
    font-weight: 700;
    margin-top: 0rpx;
}

.project__info__location {
    font-weight: 400;
    color: rgba(0,0,0,.65);
    font-size: 24rpx;
    line-height: 32rpx;
}

.project__info__bottom {
    text-align: right;
}

.project__info__bottom,.project__info__bottom--ad {
    font-size: 22rpx;
    overflow: hidden;
    height: 32rpx;
}

.image__gradient {
    position: absolute;
    height: 48rpx;
    background: linear-gradient(180deg,rgba(48,50,55,0),#303237);
    border-radius: 0 0 8rpx 8rpx;
    width: 100%;
    bottom: 0;
}

.project__img__tags {
    position: absolute;
    right: 12rpx;
    height: 40rpx;
    bottom: 16rpx;
    text-align: center;
    z-index: 3;
    display: flex;
    height: 100%;
    flex-direction: column-reverse;
}

.project__img__tags .left__tags__item {
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    display: inline-block;
    margin: 4rpx 0;
}

.left__tags__item:nth-child(1) {
    margin-bottom: 0;
}

.tags__item__img {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.ad3 {
    position: relative;
}

.ad__banner__img--big {
    width: 686rpx;
    border-radius: 8rpx;
}