.name-box {
    width: auto;
    font-weight: 700;
    color: rgba(0,0,0,.85);
    margin-left: 16rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.c-1 {
    color: #2d8cf0!important;
}

.banner {
    background: rgba(9,33,73,.64);
    position: relative;
}

.banner,.banner image {
    width: 100%;
    height: 320rpx;
}

.banner image {
    filter: blur(4px);
}

.banner image:after {
    content: "";
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 3;
    background: rgba(9,33,73,.64);
}

.banner .info {
    width: auto;
    height: 48rpx;
    overflow: hidden;
    display: flex;
    align-items: center;
    justify-content: space-between;
    position: absolute;
    bottom: 76rpx;
    left: 32rpx;
    right: 32rpx;
}

.banner .info .info-left {
    width: auto;
    font-size: 36rpx;
    font-weight: 700;
    color: #fff;
}

.banner .info .info-left .iconnew {
    margin-left: 12rpx;
}

.banner .info .info-right {
    font-size: 28rpx;
    font-weight: 400;
    color: hsla(0,0%,100%,.86);
}

page {
    padding-bottom: 100rpx;
}

.page {
    background: #fff;
    border-radius: 28rpx 28rpx 0 0;
    position: relative;
    margin-top: -36rpx;
    z-index: 1;
    box-sizing: border-box;
    overflow: hidden;
}

.content {
    display: inline-block;
    width: auto;
    height: auto;
    font-size: 30rpx;
    font-weight: 400;
    color: rgba(0,0,0,.85);
    line-height: 44rpx;
    text-indent: 2em;
    text-align: justify;
}

.last {
    padding-bottom: 0!important;
    border-bottom: 0!important;
}

.mb-6 {
    margin-bottom: 6rpx;
}

.right-btn {
    width: 168rpx;
    height: 56rpx;
    background: rgba(45,140,240,.08);
    border-radius: 28rpx;
    color: #2f8cef;
    display: flex;
    align-items: center;
    justify-content: center;
    text-align: center;
}

.right-btn .text {
    font-weight: 700;
    font-size: 26rpx;
    padding-left: 8rpx;
}

.right-btn .iconnew {
    display: inline-block;
    width: 30rpx;
    text-align: center;
    font-size: 24rpx!important;
}

.color-3 {
    color: #303237!important;
}

.flex-1 {
    flex: 1;
}

.projectDetail-bg {
    margin: 0 32rpx;
    background: #fff;
    overflow: hidden;
    position: relative;
    border-bottom: 1rpx solid rgba(0,0,0,.07);
    padding-bottom: 40rpx;
    box-sizing: border-box;
}

.projectDetail-bg .common-title {
    margin: 40rpx 0 28rpx;
    width: auto;
    height: 48rpx;
    font-size: 36rpx;
    font-weight: 700;
    color: rgba(0,0,0,.85);
    line-height: 48rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.projectDetail-bg .mb-20 {
    margin-bottom: 20rpx!important;
}

.projectDetail-title {
    padding: 24rpx 0rpx 24rpx 24rpx;
    box-sizing: border-box;
    overflow: hidden;
    display: flex;
    align-items: center;
}

.projectDetail-title .buildImg {
    width: 192rpx;
    height: 130rpx;
    background: #d8d8d8;
    border-radius: 8rpx;
}

.projectDetail-title-name {
    flex: 1;
    margin: 0 30rpx;
}

.projectDetail-title-name .area {
    color: #96989e;
    margin-top: 8rpx;
}

.projectDetail-title-name .project-tags {
    padding: 4rpx 6rpx;
    box-sizing: border-box;
    display: inline-block;
    text-align: center;
    color: #0abf79;
    margin-top: 15rpx;
    background: rgba(10,191,121,.15);
    border-radius: 8rpx;
}

.left-key {
    font-size: 30rpx;
    float: left;
    width: 160rpx;
    color: rgba(0,0,0,.45)!important;
    height: 44rpx;
    line-height: 44rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.left-key .text-val {
    display: inline-block;
    width: 140rpx;
    text-align: justify;
    text-align-last: justify;
    height: 44rpx;
}

.left-key .text-val::after {
    width: 100%;
    display: inline-block;
    content: "";
}

.right-value {
    margin-left: 170rpx;
    overflow: hidden;
    word-break: normal;
    word-wrap: break-word;
    font-size: 30rpx;
    color: rgba(0,0,0,.85);
    line-height: 44rpx;
    text-align: justify;
}

.small-title {
    padding: 30rpx 0;
    border-bottom: 1rpx solid #e2e2e2;
}

.color-style {
    font-weight: 700;
}

.mT-projecDetail-mainInfo-line {
    display: flex;
}

.projecDetail-mainInfo-line:not(:last-child) {
    margin-bottom: 12rpx;
}

.projecDetail-mainInfo-line {
    overflow: hidden;
}

.projecDetail-mainInfo-line .metroTraffic .p:not(:last-child) {
    padding-bottom: 20rpx;
}

.projecDetail-mainInfo-line .metroTraffic .p {
    display: flex;
    align-items: center;
    width: 542rpx;
    flex-wrap: nowrap;
}

.projecDetail-mainInfo-line .metroTraffic .p .icon {
    margin-top: 4rpx;
}

.projecDetail-mainInfo-line .metroTraffic .p .lineZeroIcon {
    color: #adaeb6;
}

.projecDetail-mainInfo-line .metroTraffic .p .lineTwoIcon {
    color: #ff6c17;
}

.projecDetail-mainInfo-line .metroTraffic .p .span {
    margin-left: 10rpx;
}

.projecDetail-mainInfo {
    margin: 20rpx 0 20rpx 30rpx;
}

.projecDetail-introduce-content {
    display: block;
    width: 100%;
    word-break: break-all;
    overflow: hidden;
}

.projecDetail-around {
    margin: 32rpx 32rpx 0;
    box-sizing: border-box;
    border-radius: 12rpx;
    overflow: hidden;
    color: rgba(0,0,0,.85);
    background: #fafcfe;
}

.projecDetail-around .table-flex {
    overflow: hidden;
    padding: 12rpx 0;
    box-sizing: border-box;
}

.projecDetail-around .table-flex:first-child {
    width: auto;
    height: 76rpx;
    line-height: 76rpx;
    font-weight: 700;
    background: #f1f8ff;
    padding: 0!important;
}

.projecDetail-around .table-flex:first-child .table-col {
    height: 76rpx;
    line-height: 76rpx;
}

.projecDetail-around .table-col {
    width: 31%;
    margin-left: 2.3333%;
    float: right;
    word-wrap: break-word;
    line-height: 40rpx;
}

.errorCorrection {
    width: auto;
    height: auto;
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(0,0,0,.45);
    line-height: 34rpx;
    margin: 40rpx 32rpx 80rpx;
    padding-bottom: env(safe-area-inset-bottom);
}

.errorCorrection .blue-color {
    color: #2d8cf0;
}

.qrCode {
    position: absolute;
    right: 0;
    top: 120rpx;
    overflow: hidden;
}

.qrCode,.qrcode-img {
    width: 136rpx;
    height: 136rpx;
}

.projecDetail-mainInfo-content {
    min-height: 296rpx;
}

.skeletons .projectDetail-title-name .skeletons-rect {
    width: 100%;
    height: 30rpx;
}

.skeletons .projecDetail-mainInfo-line.skeletons-rect {
    margin-bottom: 0;
}

.auth-report .authorization__button {
    text-align: left;
}