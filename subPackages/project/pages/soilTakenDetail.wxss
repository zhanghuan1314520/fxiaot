page {
    background: #f2f2f2;
}

.page,page {
    padding-bottom: 130rpx;
}

.share {
    background: transparent;
    width: 100%;
    height: 100%;
    position: absolute;
}

#map-container {
    width: 100%;
    height: 350rpx;
    overflow: hidden;
}

#map-container #map {
    width: 100%;
    height: 100%;
}

#map-container #map .showMoreLocal {
    width: 224rpx;
    height: 46rpx;
    background: rgba(0,0,0,.5);
    position: absolute;
    right: 24rpx;
    top: 30rpx;
    display: inline-block;
    text-align: center;
    line-height: 46rpx;
    border-radius: 46rpx;
    color: #fff;
}

.container-box {
    margin-bottom: 16rpx;
    padding: 28rpx 22rpx;
    box-sizing: border-box;
    background: #fff;
    position: relative;
    min-height: 270rpx;
}

.container-box-line {
    overflow: hidden;
    margin-bottom: 20rpx;
}

.container-box-line .left-key {
    float: left;
    width: 160rpx;
    color: #96989e;
    line-height: 34rpx;
}

.container-box-line .left-key .tips {
    color: #2d8cf0;
    position: relative;
    top: 2rpx;
}

.container-box-line .right-value {
    margin-left: 160rpx;
    overflow: hidden;
    word-break: normal;
    word-wrap: break-word;
    color: #303237;
    line-height: 34rpx;
}

.container-box-line:last-child {
    margin-bottom: 0;
}

.footer {
    position: fixed;
    left: 0;
    bottom: 0;
    height: 124rpx;
    z-index: 999;
    background: hsla(0,0%,100%,.95);
    box-shadow: 0 -4rpx 12rpx 0 rgba(21,24,45,.03);
    padding: 0 24rpx;
    box-sizing: border-box;
}

.footer,.footer .footer-btn {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}

.footer .footer-btn {
    height: 88rpx;
    border-radius: 4rpx;
    color: #fff;
    font-weight: 700;
}

.footer .footer-btn .text {
    margin-left: 10rpx;
}

.footer .footer-btn image {
    width: 48rpx;
    height: 48rpx;
}

.footer .color-left {
    background: #2f8cef;
}

.footer .color-right {
    margin-left: 22rpx;
    background: linear-gradient(52deg,#ff8e5c,#ff5431);
}

.canvas-bg {
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 999;
    background: rgba(0,0,0,.65);
    padding-bottom: env(safe-area-inset-bottom);
}

.canvas-bg,.canvas-bg .share-canvas {
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}

.canvas-bg .share-canvas {
    flex-direction: column;
    text-align: center;
}

.canvas-bg .share-canvas .close-canvas-btn {
    display: inline-block;
    padding: 10rpx 0 0;
    text-align: center;
    box-sizing: border-box;
    width: 100%;
}

.canvas-bg .share-canvas .close-canvas-btn .tip-content {
    position: relative;
    padding-bottom: 26rpx;
    overflow: hidden;
    height: 96rpx;
}

.canvas-bg .share-canvas .close-canvas-btn .tip-content image {
    position: absolute;
    top: 0;
    left: 50%;
    width: 540rpx;
    height: 96rpx;
    transform: translateX(-50%);
}

.canvas-bg .share-canvas .close-canvas-btn .tip-content .save-tips {
    position: absolute;
    top: 40rpx;
    left: 50%;
    transform: translateX(-50%);
    width: 100%;
}

.canvas-bg .share-canvas .close-canvas-btn .close-iconnew {
    width: 64rpx;
    height: 64rpx;
    display: inline-block;
    background: transparent;
}

.permissions-settings {
    width: 606rpx;
    background: #fff;
    border-radius: 8rpx;
    text-align: center;
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    z-index: 5;
}

.permissions-settings .tips-content {
    padding-top: 54rpx;
    padding-bottom: 8rpx;
    line-height: 50rpx;
    z-index: 6;
}

.permissions-settings .text {
    line-height: 50rpx;
    padding-bottom: 48rpx;
    z-index: 6;
}

.permissions-settings .bottom-btn {
    border-top: 1rpx solid rgba(0,0,0,.1);
    height: 100rpx;
    line-height: 100rpx;
    align-items: center;
    overflow: hidden;
}

.permissions-settings .bottom-btn .cancle {
    text-align: center;
    width: 50%;
    float: left;
    border-right: 1rpx solid #e2e4ea;
    color: #606266;
    height: 100%;
    box-sizing: border-box;
}

.permissions-settings .bottom-btn .sure-btn {
    height: 100%;
    text-align: center;
    width: 50%;
    float: right;
    color: #2f8cef;
    background: transparent;
}

.hide-bg {
    background: rgba(0,0,0,.65);
    z-index: 2;
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
}

.qr-code-content {
    position: absolute;
    top: 16rpx;
    right: 20rpx;
}

.qr-code-content .qr-code {
    width: 140rpx;
    height: 140rpx;
    background: rrgba(216,216,216,1);
    margin-bottom: 12rpx;
}

.qr-code-content .text {
    line-height: 28rpx;
    text-align: center;
}

.page-control {
    position: fixed;
    right: 0;
    bottom: 184rpx;
    z-index: 99;
    margin-right: 24rpx;
}

.page-control,.page-control .btn {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
}

.page-control .btn {
    height: 84rpx;
    width: 84rpx;
    margin-top: 20rpx;
    color: #fff;
    line-height: 1;
    border-radius: 50%;
}

.page-control .btn .iconnew {
    line-height: 1;
}

.page-control .share {
    background: rgba(47,140,239,.9);
    box-shadow: 0 4rpx 16rpx 0 rgba(47,140,239,.3);
}

.page-control .backhome {
    background: rgba(35,205,148,.98);
    box-shadow: 0 4rpx 16rpx 0 rgba(35,205,148,.3);
}