page {
    background: #f7f7f7;
}

.view-man {
    overflow: hidden;
    padding: 24rpx;
    display: -webkit-flex;
    flex-wrap: wrap;
    justify-content: flex-start;
}

.view-man .person {
    width: 20%;
    margin-bottom: 24rpx;
    text-align: center;
}

.view-man .person image {
    border: 2rpx solid rgba(48,55,64,.03);
    width: 108rpx;
    height: 108rpx;
    border-radius: 108rpx;
    margin-bottom: 10rpx;
}

.view-man .person .time {
    font-size: 24rpx;
    color: #b5b6bb;
    line-height: 24rpx;
}