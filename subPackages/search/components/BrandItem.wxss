.brand {
    justify-content: space-between;
    padding: 24rpx 0;
    border-bottom: 1rpx solid rgba(21,24,45,.05);
}

.brand,.brand-info {
    display: flex;
    align-items: center;
}

.brand-name {
    color: #383f51;
    font-size: 32rpx;
    font-weight: 700;
}

.brand-icon {
    width: 32rpx;
    height: 28rpx;
    margin-left: 10rpx;
}

.brand-logo {
    width: 214rpx;
    height: 156rpx;
    border-radius: 10rpx;
    margin-right: 26rpx;
    border: 1rpx solid #edeff2;
}

.brand-entry {
    width: 126rpx;
    height: 50rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #2d8cf0;
    border-radius: 8rpx;
    font-size: 24rpx;
    color: #fff;
}